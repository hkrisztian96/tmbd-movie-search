{
    "extends": [
      "tslint:recommended",
      "tslint-react"
    ],
    "rules": {
      "arrow-parens": false,
      "object-literal-sort-keys":  false,
      "jsx-no-lambda": false,
      "jsx-no-multiline-js": false,
      "no-empty-interface": false,
      "max-line-length": [
        true,
        {
          "limit": 150,
          "ignore-pattern": "^import |^export {(.*?)}"
        }
      ],
      "member-ordering": false,
      "semicolon": false,
      "interface-over-type-literal": false,
      "no-console": [
        true,
        "log"
      ]
    }
}
  